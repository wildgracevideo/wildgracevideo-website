{
    "name": "wgv-website",
    "devDependencies": {
        "@nuxt/devtools": "1.0.8",
        "@nuxt/image": "1.7.0",
        "@nuxt/scripts": "0.9.0",
        "@nuxt/ui": "2.15.2",
        "@nuxtjs/google-fonts": "3.1.3",
        "@nuxtjs/sitemap": "5.0.5",
        "@types/jsonwebtoken": "9.0.5",
        "@types/node-fetch": "2.6.10",
        "@types/uuid": "9.0.8",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.1",
        "@unhead/schema-org": "1.7.4",
        "@vue/eslint-config-typescript": "12.0.0",
        "autoprefixer": "10.4.16",
        "dotenv": "16.4.5",
        "drizzle-kit": "0.24.2",
        "eslint": "8.55.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-drizzle": "0.2.3",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-vue": "9.19.2",
        "eslint-plugin-vuejs-accessibility": "2.2.0",
        "nuxt": "3.11.2",
        "nuxt-schema-org": "3.2.3",
        "nuxt-simple-robots": "3.1.5",
        "postcss": "8.4.38",
        "prettier": "3.1.1",
        "prettier-plugin-tailwindcss": "0.5.9",
        "shx": "0.3.4",
        "tailwindcss": "3.4.3",
        "typescript": "5.3.3"
    },
    "private": true,
    "scripts": {
        "set-deployed-cms-config": "shx sed -i \"s/http:\\/\\/localhost:3000/https:\\/\\/www\\.wildgracevideo\\.com/g\" ./public/admin/cms/config.yml && shx sed -i \"s/local_backend\\: true/local_backend: false/g\" ./public/admin/cms/config.yml",
        "set-local-cms-config": "shx sed -i \"s/https:\\/\\/www\\.wildgracevideo\\.com/http:\\/\\/localhost:3000/g\" ./public/admin/cms/config.yml && shx sed -i \"s/local_backend: false/local_backend: true/g\" ./public/admin/cms/config.yml",
        "build": "npm run set-deployed-cms-config  &> '/dev/null' && nuxt build",
        "dev": "npm run set-local-cms-config &> '/dev/null' && nuxt dev --host",
        "start": "node .output/server/index.mjs",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "analyze": "nuxt build --analyze",
        "lint:js": "eslint . --ext .ts,.vue --ignore-path .prettierignore",
        "lint:prettier": "prettier --check .",
        "lint": "npm run lint:js && npm run lint:prettier",
        "lintfix": "prettier --write --list-different . && npm run lint:js --fix",
        "validate": "npm run lint && npm run build",
        "dbview": "npx drizzle-kit studio",
        "prd-dbview": "npx drizzle-kit studio --config prd.drizzle.config.ts",
        "migrate": "MIGRATE_ENV=dev npx tsx migrate.ts",
        "prd-migrate": "MIGRATE_ENV=prd npx tsx migrate.ts"
    },
    "type": "module",
    "dependencies": {
        "@auth/drizzle-adapter": "0.8.0",
        "@aws-sdk/client-s3": "3.609.0",
        "@aws-sdk/client-sesv2": "3.609.0",
        "@aws-sdk/s3-request-presigner": "3.609.0",
        "@headlessui/vue": "1.7.20",
        "@heroicons/vue": "2.0.18",
        "@libsql/client": "0.5.6",
        "@lucia-auth/adapter-drizzle": "1.1.0",
        "@nuxt/content": "2.12.0",
        "@sendgrid/eventwebhook": "7.7.0",
        "@sendgrid/mail": "7.7.0",
        "@types/grecaptcha": "3.0.5",
        "@types/markdown-it": "13.0.7",
        "@types/node": "20.10.5",
        "@uppy/aws-s3": "4.1.0",
        "@uppy/core": "4.2.0",
        "@uppy/dashboard": "4.1.0",
        "api": "6.1.1",
        "countup.js": "2.8.0",
        "drizzle-orm": "0.33.0",
        "lucia": "3.2.0",
        "markdown-it": "14.0.0",
        "marked": "11.1.0",
        "maska": "2.1.10",
        "shaka-player": "4.11.0",
        "stripe": "14.10.0",
        "uuid": "9.0.1",
        "yup": "1.4.0"
    }
}
