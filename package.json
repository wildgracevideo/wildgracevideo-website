{
    "name": "nuxt-app",
    "devDependencies": {
        "@nuxt/devtools": "1.0.8",
        "@nuxtjs/google-fonts": "3.1.3",
        "@nuxtjs/sitemap": "5.0.5",
        "@sidebase/nuxt-auth": "0.6.7",
        "@types/jsonwebtoken": "9.0.5",
        "@types/node-fetch": "2.6.10",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.1",
        "@unhead/schema-org": "1.7.4",
        "@vue/eslint-config-typescript": "12.0.0",
        "autoprefixer": "10.4.16",
        "eslint": "8.55.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-vue": "9.19.2",
        "eslint-plugin-vuejs-accessibility": "2.2.0",
        "nuxt": "3.10.2",
        "nuxt-schema-org": "3.2.3",
        "nuxt-simple-robots": "3.1.5",
        "postcss": "8.4.29",
        "prettier": "3.1.1",
        "prettier-plugin-tailwindcss": "0.5.9",
        "prisma": "5.10.1",
        "shx": "0.3.4",
        "tailwindcss": "3.4.0",
        "typescript": "5.3.3"
    },
    "private": true,
    "scripts": {
        "set-deployed-cms-config": "shx sed -i \"s/http:\\/\\/localhost:3000/https:\\/\\/www\\.wildgracevideo\\.com/g\" ./public/admin/cms/config.yml && shx sed -i \"s/local_backend\\: true/local_backend: false/g\" ./public/admin/cms/config.yml",
        "set-local-cms-config": "shx sed -i \"s/https:\\/\\/www\\.wildgracevideo\\.com/http:\\/\\/localhost:3000/g\" ./public/admin/cms/config.yml && shx sed -i \"s/local_backend: false/local_backend: true/g\" ./public/admin/cms/config.yml",
        "build": "npm run set-deployed-cms-config  &> '/dev/null' && nuxt build",
        "dev": "npm run set-local-cms-config &> '/dev/null' && nuxt dev --host",
        "start": "node .output/server/index.mjs",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "analyze": "nuxt build --analyze",
        "lint:js": "eslint . --ext .ts,.vue --ignore-path .prettierignore",
        "lint:prettier": "prettier --check .",
        "lint": "npm run lint:js && npm run lint:prettier",
        "lintfix": "prettier --write --list-different . && npm run lint:js --fix",
        "validate": "npm run lint && npx prisma generate && npm run build",
        "dbview": "npx prisma studio"
    },
    "type": "module",
    "dependencies": {
        "@aws-sdk/client-sesv2": "3.436.0",
        "@headlessui/vue": "1.7.16",
        "@heroicons/vue": "2.0.18",
        "@next-auth/prisma-adapter": "1.0.7",
        "@nuxt/content": "2.12.0",
        "@prisma/client": "5.10.1",
        "@sendgrid/eventwebhook": "7.7.0",
        "@sendgrid/mail": "7.7.0",
        "@types/grecaptcha": "3.0.5",
        "@types/markdown-it": "13.0.7",
        "@types/node": "20.10.5",
        "api": "6.1.1",
        "aws-sdk": "2.1459.0",
        "markdown-it": "14.0.0",
        "marked": "11.1.0",
        "maska": "2.1.10",
        "stripe": "14.10.0",
        "zod-prisma-types": "3.0.0"
    },
    "overrides": {
        "next-auth": "4.21.1"
    }
}
